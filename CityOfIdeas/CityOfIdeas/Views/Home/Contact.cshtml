@{
    ViewData["Title"] = "Index";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <title>Contact V5</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/Vragenlijst/animate/animate.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/Vragenlijst/css-hamburgers/hamburgers.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/Vragenlijst/animsition/css/animsition.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/Vragenlijst/select2/select2.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/Vragenlijst/daterangepicker/daterangepicker.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/Vragenlijst/noui/nouislider.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/css/Contact/util.css">
    <link rel="stylesheet" type="text/css" href="~/css/Contact/contact.css">
    <link rel="stylesheet" type="text/css" href="~/css/util.css">
    <link rel="stylesheet" type="text/css" href="~/css/main.css">

    <!--===============================================================================================-->
</head>
<body>

    <div class="limiter" id="backgroundLogin">
        <div class="container-contact100">
            <div class="wrap-contact100">
                <form class="contact100-form validate-form">
                    <span class="login100-form-title p-b-55">
                        Contact Us
                    </span>

                    <div style=" width: 100%; position: relative; padding: 10px 30px 9px 22px; margin-bottom: 20px;" data-validate="Please Type Your Name">
                        <input class="input100" type="text" name="name" placeholder="Enter Your Name">
                        <span class="symbol-input100">
                            <span class="lnr lnr-envelope"></span>
                        </span>
                    </div>

                    <div style=" width: 100%; position: relative; padding: 10px 30px 9px 22px; margin-bottom: 20px;" data-validate="Enter Your Email (e@a.x)">
                        <input class="input100" type="text" name="email" placeholder="Enter Your Email ">
                    </div>

                    <div style=" width: 100%; position: relative; padding: 10px 30px 9px 22px; margin-bottom: 20px;">
                        <input class="input100" type="text" name="phone" placeholder="Enter Number Phone">
                    </div>




                    <div style=" width: 100%; position: relative; padding: 10px 30px 9px 22px; margin-bottom: 20px;" data-validate="Please Type Your Message">
                        <textarea class="input100" name="message" placeholder="Your message here..."></textarea>
                    </div>

                    <div class="container-contact100-form-btn">
                        <button class="btn btn-primary btn-xl js-scroll-trigger" style="width:15rem; font-size:15px;">
                            <span>
                                Submit
                                <i class="fa fa-long-arrow-right m-l-7" aria-hidden="true"></i>
                            </span>
                        </button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</body>
</html>

